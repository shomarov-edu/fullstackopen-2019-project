{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "prisma",
      "image": "shomarov/fullstackopen-2019-project-prisma",
      "hostname": "prisma",
      "essential": "true",
      "memory": 128
    },
    {
      "name": "api",
      "image": "shomarov/fullstackopen-2019-project-api",
      "hostname": "api",
      "essential": "false",
      "links": ["prisma"],
      "memory": 128
    },
    {
      "name": "client",
      "image": "shomarov/fullstackopen-2019-project-client",
      "hostname": "client",
      "essential": "false",
      "links": ["api"],
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "shomarov/fullstackopen-2019-project-nginx",
      "hostname": "nginx",
      "essential": "true",
      "links": ["prisma", "api", "client"],
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    }
  ]
}
