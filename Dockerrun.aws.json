{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "prisma",
      "image": "shomarov/fullstackopen-2019-project-prisma",
      "hostname": "prisma",
      "essential": "false",
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 4466,
          "containerPort": 4466
        }
      ]
    },
    {
      "name": "api",
      "image": "shomarov/fullstackopen-2019-project-api",
      "hostname": "api",
      "essential": "false",
      "links": ["prisma"],
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 4000,
          "containerPort": 4000
        }
      ]
    },
    {
      "name": "client",
      "image": "shomarov/fullstackopen-2019-project-client",
      "hostname": "client",
      "essential": "true",
      "memory": 128,
      "links": ["api"],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    }
  ]
}
